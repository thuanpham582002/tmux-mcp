{
  "name": "tmux-mcp",
  "version": "0.1.3-raw",
  "description": "MCP Server for interfacing with tmux sessions",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "check-release": "npm run build && npm publish --dry-run",
    "release": "npm run build && npm publish"
  },
  "bin": {
    "tmux-mcp": "build/index.js"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "mcp",
    "tmux",
    "claude"
  ],
  "author": "nickgnd",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.2",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickgnd/tmux-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/nickgnd/tmux-mcp/issues"
  },
  "homepage": "https://github.com/nickgnd/tmux-mcp#readme"
}
